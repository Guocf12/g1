<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.briup.apps.sms.dao.UserRoleDao">
  <select id="selectAll" resultType="com.briup.apps.sms.bean.UserRole">
    select * from sms_user_role;
  </select>
   <insert id="insert">
  	insert into sms_user_role values(null,#{user_id},#{role_id});
  </insert>

  <update id="update">
  	update sms_user_role;
	set user_id=#{user_id},
	    role_id#{role_id}
	where id = #{id}
  </update>

  <delete id="deleteById">
  	delete from sms_user_role where id = #{id}
  </delete>


  
</mapper>
